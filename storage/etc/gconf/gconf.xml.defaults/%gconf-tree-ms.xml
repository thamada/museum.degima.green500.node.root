					</entry>
					<entry name="location0">
						<local_schema>
						</local_schema>
					</entry>
				</dir>
			</dir>
			<dir name="gedit-2">
				<dir name="preferences">
					<dir name="editor">
						<dir name="font">
							<entry name="editor_font">
								<local_schema>
								</local_schema>
							</entry>
						</dir>
					</dir>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
